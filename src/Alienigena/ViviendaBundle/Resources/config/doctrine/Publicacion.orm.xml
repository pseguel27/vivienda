<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Alienigena\ViviendaBundle\Entity\Publicacion" table="publicacion">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="publicacion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="idUsuario" type="integer" column="id_usuario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idDetalle" type="integer" column="id_detalle" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTipoServicio" type="integer" column="id_tipo_servicio" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTipoPublicacion" type="integer" column="id_tipo_publicacion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTipoPropiedad" type="integer" column="id_tipo_propiedad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idCiudad" type="integer" column="id_ciudad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTipoValor" type="integer" column="id_tipo_valor" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="valor" type="float" column="valor" precision="10" scale="0" nullable="false"/>
    <field name="estado" type="integer" column="estado" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numDormitorio" type="integer" column="num_dormitorio" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="superficieConstruida" type="integer" column="superficie_construida" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="superficieTerreno" type="integer" column="superficie_terreno" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaIngreso" type="datetime" column="fecha_ingreso" nullable="false"/>
    <field name="fechaModificacion" type="datetime" column="fecha_modificacion" nullable="false"/>
  </entity>
</doctrine-mapping>
